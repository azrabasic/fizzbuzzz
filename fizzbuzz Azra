print "Enter number between 1 and 100!"

end = raw_input ("Enter the number: ")

end = int (end)

for number in range (1, end+1):
    if number % 3 == 0 and number % 5 == 0:
        print "fizzbuzz"
    elif number % 3 == 0:
        print "fizz"
    elif number % 5 == 0:
        print "buzz"
    else:
        print number


